  "scripts": {
    "test": "mocha --compilers js:babel/register",
    "lint": "eslint ./",
    "clean": "rm src/bundle.js || true",
    "prebuild": "npm run clean -s",
    "build": "browserify src/components/main.js -o src/bundle.js",
    "prestart": "npm run build && npm run test && npm run lint",
    "watch": "watchify src/components/main.js -o src/bundle.js -dv",
    "dev": "npm run watch & node app.js",
    "start": "node app.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "stage": 1,
          "sourceMaps": true,
          "optional": [
            "runtime"
          ]
        }
      ]
    ]
  },
