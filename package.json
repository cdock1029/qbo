    "classnames": "latest",
    "core-js": "^0.9.16",
    "crumb": "latest",
    "eslint": "latest",
    "eslint-plugin-react": "latest",
    "flummox": "latest",
    "flux": "latest",
    "functional.js": "latest",
    "good": "latest",
    "good-console": "latest",
    "handlebars": "latest",
    "hapi": "latest",
    "hapi-auth-cookie": "latest",
    "hapi-require-https": "latest",
    "hoek": "latest",
    "immutable": "latest",
    "mocha": "latest",
    "moment": "latest",
    "node-parse-api": "git://github.com/cdock1029/node-parse-api.git",
    "node-quickbooks": "git://github.com/cdock1029/node-quickbooks.git#queried-count",
    "object-assign": "latest",
    "parse": "latest",
    "parse-react": "latest",
    "path": "^0.11.14",
    "q": "latest",
    "qs": "latest",
    "ramda": "latest",
