{
  "dependencies": {
    "accounting": "^0.4.1",
    "aws-sdk": "^2.1.26",
    "bell": "^2.4.0",
    "boom": "^2.7.1",
    "classnames": "^1.2.2",
    "crumb": "^4.0.3",
    "flux": "^2.0.3",
    "functional.js": "^0.6.15",
    "good": "^5.1.2",
    "good-console": "^4.1.0",
    "handlebars": "^3.0.3",
    "hapi": "^8.4.0",
    "hapi-auth-cookie": "^2.1.0",
    "hoek": "^2.12.0",
    "immutable": "^3.7.2",
    "moment": "^2.10.2",
    "node-parse-api": "git://github.com/cdock1029/node-parse-api.git",
    "node-quickbooks": "git://github.com/cdock1029/node-quickbooks.git#queried-count",
    "object-assign": "^2.0.0",
    "parse": "^1.4.2",
    "parse-react": "^0.1.4",
    "q": "^1.3.0",
    "qs": "^2.4.1",
    "react": "^0.13.2",
    "react-bootstrap": "^0.21.0",
    "react-spinkit": "^1.0.2",
    "request": "^2.55.0",
    "underscore": "^1.8.3",
    "wreck": "^5.5.1",
    "yar": "*",
    "browserify": "^9.0.8",
    "jshint": "latest",
    "eslint": "latest",
    "reactify": "^1.1.0",
    "babel": "^4.7.13"
  },
  "main": "src/main.js",
  "scripts": {
    "env": "env",
    "hint": "jshint **.js",
    "lint": "eslint **.js",
    "dev": "browserify src/main.js -o src/bundle.js -v -d",
    "buildServer": "babel src/utils --out-dir lib/utils",
    "buildUi": "babel src/components --out-dir lib/components && browserify src/main.js -o src/bundle.js",
    "clean": "rm src/bundle.js; rm -r lib",
    "prebuild": "npm run clean -s",
    "build": "npm run buildServer && npm run buildUi"
  },
  "browserify": {
    "transform": [
      [
        "reactify", {
          "es6": true
        }
      ]
    ]
  },
  "devDependencies": {
    "jsx-loader": "*",
    "react-tools": "^0.13.2",
    "watchify": "^3.1.2"
  },
  "jshintConfig": {
    "node": true,
    "esnext": true,
    "unused": "vars",
    "eqeqeq": true,
    "forin": true,
    "futurehostile": true,
    "latedef": true,
    "nonew": true,
    "singleGroups": true,
    "undef": true
  },
  "engines": {
    "iojs": ">=2.0.1"
  }
}
