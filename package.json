    "react-spinkit": "latest",
    "request": "latest",
    "underscore": "latest",
    "wreck": "latest",
    "yar": "latest"
  },
  "scripts": {
    "test": "mocha --compilers js:babel/register app/test",
    "lint": "eslint .",
    "clean-server": "rm -r app/node_modules/server/* & rm -r app/node_modules/lib/* & rm npm-debug.log.* & rm server.output || true",
    "clean-client": "rm app/node_modules/public/js/* || true",
    "clean": "npm run clean-server & npm run clean-client || true",
    "prebuild": "npm run clean -s",
    "build-server": "babel -d app/node_modules/server app/source/server -s && babel -d app/node_modules/lib app/source/lib -s",
    "build-client": "browserify app/source/client/components/main.js -o app/node_modules/public/js/bundle.js",
    "build": "npm run build-server & npm run build-client",
    "prestartNO": "npm run build && npm run test && npm run lint",
    "watch-client": "watchify app/source/client/components/main.js -o 'exorcist app/node_modules/public/js/bundle.js.map -u /js/bundle.js.map > app/node_modules/public/js/bundle.js' -dv",
    "watch-server": "babel -d app/node_modules/server app/source/server -s -w & babel -d app/node_modules/lib app/source/lib -s -w",
    "watch": "npm run watch-client & npm run watch-server",
    "dev": "npm run watch & sleep 7.0 && nodemon --delay 1.900ms --watch app/node_modules/server --watch app/node_modules/lib --watch app/index.js > server.output",
    "start": "node app/index.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "stage": 1,
          "sourceMaps": true,
          "optional": [
            "runtime"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "exorcist": "^0.4.0",
    "gulp": "^3.9.0",
    "watchify": "latest"
  }
}
